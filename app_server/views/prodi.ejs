<h1>Data Prodi</h1>
<table class="table">
  <tr>
    <th>Nama Prodi</th>
    <th>Fakultas</th>
    <th>Singkatan</th>
  </tr>
  <% prodi.forEach(data => { %>
  <tr>
    <td><%= data.nama %></td>
    <td><%= data.fakultas_id.nama %></td>
    <td><%= data.singkatan %></td>
  </tr>
  <% }) %>
</table>
